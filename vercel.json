{
    "builds": [
        {
            "src": "mysite/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        }
    ],
    "ignoreFiles": [
        "**/tests/**",
        "**/__pycache__/**",
        "**/*.md",
        "**/*.pyc",
        "**/node_modules/**",
        "**/notebooks/**"
    ]
}